import pandas as pd
import csv
import numpy as np
import matplotlib.pyplot as plt

# load the data
data = pd.read_csv('thesis_dataset_incomplete.csv')

# sort data according to PatientIDNumber 
sorted_data = data.sort_values(by='PatientIDNumber')

# Get one patient
patient1 = sorted_data.loc[sorted_data['PatientIDNumber']==383]
sorted_patient1 = patient1.sort_values(by='TreatmentStart')


plt.plot(pd.to_datetime(sorted_patient1.TreatmentStart),sorted_patient1['LitersProcessed'])
